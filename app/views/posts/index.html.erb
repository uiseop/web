<p id="notice"><%= notice %></p>

<h1>게시판</h1>

    <div class="jumbotron">

      <div class="container">

        <table class="table table-striped">
  <thead>
    <tr>
      <th>제목</th>
      <th>작성자</th>
      <th>작성된 시간</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
       <td><%= link_to post.title, post %></td>
        <td><%= post.user.email %></td>
        <td><%= post.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
      </div>
    </div>

<br>
