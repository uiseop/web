<p id="notice"><%= notice %></p>

<h1>내 페이지</h1>

    <div class="jumbotron">

      <div class="container">
<table class="table table-striped">
  <thead>
    <tr>
      <th>내가 작성한 글 목록</th>
     <th>작성자</th>
      <th>작성된 시간</th>
      <th colspan="3"></th>
    </tr>
  </thead>


  <tbody>
  
   <% @myposts.each do |post| %>
   <% if current_user.email == post.user.email %>
<tr>
       <td><%= link_to post.title, post %></td><br>
        <td><%= post.user.email %></td>
        <td><%= post.created_at %></td>
</tr>

<% end %>
<% end %>
  </tbody>
</table>
<br>
      </div>
    </div>

<a href="/">뒤로가기</a>

